FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 14.855;

boundaryField
{
    Outlet
    {
        type            zeroGradient;
    }
    Inlet
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.0007;
        value           uniform 0;
    }
    Others
    {
        type            empty;
    }
    walls
    {
        type            epsilonWallFunction;
        value           uniform 0;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 14.855;
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform 14.855;
    }
}


