FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.001;

boundaryField
{
    Inlet
    {
	type              turbulentIntensityKineticEnergyInlet;
	intensity       0.02;
        value           uniform 0;
    }
    Outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            kqRWallFunction;
        value           uniform 0;
    }
	Others
	{
	type					empty;
	}
}

