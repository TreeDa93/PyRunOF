FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];



Ical				0.04310502051153895;
kp 				0.0027870641899502904;

internalField   uniform $kp;  

boundaryField
{
    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       $Ical;
		value           uniform $kp;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform $kp;
        value           uniform $kp;
    }
    ".*"
    {
        type            kqRWallFunction;
        value           $internalField;
    }
}


