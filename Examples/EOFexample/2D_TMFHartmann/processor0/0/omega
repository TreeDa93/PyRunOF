FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.001;

boundaryField
{
    Outlet
    {
        type            zeroGradient;
    }
    Inlet
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength    0.007;
        value           uniform 0;
    }
    Others
    {
        type            empty;
    }
    walls
    {
        type            omegaWallFunction;
        value           uniform 0;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 0.001;
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform 0.001;
    }
}


// ************************************************************************* //
