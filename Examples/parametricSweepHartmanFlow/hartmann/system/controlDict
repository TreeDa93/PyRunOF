startTime       startTime_var;

stopAt          endTime;

endTime         endTime_var;

deltaT          0.005;

writeControl    adjustableRunTime;

writeInterval   writeInterval_var;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           1;

maxDeltaT       0.01;

