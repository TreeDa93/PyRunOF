FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U_old;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Outlet
    {
        type            zeroGradient;
    }
    Inlet
    {
        type            fixedProfile;
        profile         polynomial
        
2
(
((10.5 0 0) (0 0 0))
((-4200 0 0) (2 0 0))
)
;
        direction       (0 0 1);
        origin          0;
        value           uniform (10.5 0 0);
    }
    Others
    {
        type            empty;
    }
    walls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


