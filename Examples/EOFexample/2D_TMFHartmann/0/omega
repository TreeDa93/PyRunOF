FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.001;

boundaryField
{
    Inlet
    {
	//type		fixedValue;
	type            turbulentMixingLengthFrequencyInlet;
	mixingLength	  0.007;
	//type            omegaWallFunction;
        //type            fixedValue;
        value           uniform 0;
    }
    Outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            omegaWallFunction;
        value           uniform 0;
    }
	Others
	{
	type					empty;
	}
}

