FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      v2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.25;

boundaryField
{
    Outlet
    {
        type            zeroGradient;
    }
    Inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    Others
    {
        type            empty;
    }
    walls
    {
        type            v2WallFunction;
        value           uniform 0.25;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 0.25;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 0.25;
    }
}


