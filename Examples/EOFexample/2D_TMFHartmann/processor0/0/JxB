FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      JxB;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Outlet
    {
        type            zeroGradient;
    }
    Inlet
    {
        type            zeroGradient;
    }
    Others
    {
        type            empty;
    }
    walls
    {
        type            zeroGradient;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


