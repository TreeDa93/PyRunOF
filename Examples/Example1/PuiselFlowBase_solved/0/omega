FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];



L1		0.0005747336068205194; //mixingLength
omegap  	167.7050983124842; //specific dissipation rate

internalField   uniform $omegap;

boundaryField
{
    inlet
    {
		type 		turbulentMixingLengthFrequencyInlet; 
		mixingLength      $L1;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    ".*"
    {
        type            omegaWallFunction;
        value           $internalField;
    }
}



