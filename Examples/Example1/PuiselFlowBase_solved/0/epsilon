FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];


ep				0.042066438658093584;
L1		0.0005747336068205194; //mixingLength

internalField   uniform $ep; 

boundaryField
{
    outlet
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      $internalField;
    }
    inlet
    {
        type            turbulentMixingLengthDissipationRateInlet;
		mixingLength      $L1;
        value           $internalField;
    }
    ".*"
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
}


// ************************************************************************* //
