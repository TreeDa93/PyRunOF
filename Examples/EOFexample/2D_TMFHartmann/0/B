FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      B;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -2 0 0 -1 0];

internalField   uniform (0 0 0);

boundaryField
{
    Outlet
    {
        type            zeroGradient;
        value           uniform (0 0 0);
    }
    Inlet
    {
        type            zeroGradient;
        value           uniform (0 0 0);
    }
    walls
    {
        type            zeroGradient;
        value           uniform (0 0 0);
    }
	Others
	{
	type					empty;
	}
}


